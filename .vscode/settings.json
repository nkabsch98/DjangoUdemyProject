{
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "python.formatting.provider": "none",
    "python.languageServer": "Pylance",
    "files.associations": {
        "**/*.html": "html",
        "**/templates/**/*.html": "django-html",
    },
    "emmet.includeLanguages": {"django-html": "html"},
}